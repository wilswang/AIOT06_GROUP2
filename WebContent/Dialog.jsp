<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>

    <div class="modal type-warning fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  		<div class="modal-dialog" role="document">
    		<div class="modal-content">
      			<div class="modal-header" style="color:#fff;background: #f0ad4e;">
        			<h4 class="modal-title" id="exampleModalLabel" style="display:inline-block">Warning Message</h4>
        			<button type="button" class="close" data-dismiss="modal" aria-label="Close">
          				<span aria-hidden="true">&times;</span>
        			</button>
      			</div>
      			<div class="modal-body">
      				<span id="msg"></span> <span id="countdown"></span>
      				
      			</div>
      			<div class="modal-footer">
        			<button type="button" class="btn btn-warning" data-dismiss="modal" >Close</button>
      			</div>
    		</div>
  		</div>
	</div>
